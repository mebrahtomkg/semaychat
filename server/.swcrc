{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false
    },
    "target": "es2022",
    "keepClassNames": true,
    "transform": {
      "useDefineForClassFields": false,
      "optimizer": {
        "simplify": false
      }
    }
  },
  "module": {
    "type": "commonjs",
    "noInterop": false
  },
  "minify": false
}
