{
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "dynamicImport": true,
      "decorators": false
    },
    "target": "es2024",
    "keepClassNames": true,
    "transform": {
      "useDefineForClassFields": false,
      "optimizer": {
        "simplify": false
      }
    },
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "module": {
    "type": "es6",
    "resolveFully": true,
    "outFileExtension": "js"
  },
  "minify": false
}
